>\r\n    </v-row>\r\n    <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\r\n      <v-text-field\r\n        v-model=\"name\"\r\n        :rules=\"nameRules\"\r\n        label=\"Name\"\r\n        required\r\n        outlined\r\n      ></v-text-field>\r\n\r\n      <v-text-field\r\n        v-model=\"password\"\r\n        :rules=\"passwordRules\"\r\n        label=\"Password\"\r\n        type=\"password\"\r\n        required\r\n        outlined\r\n      ></v-text-field>\r\n\r\n      <div style=\"text-align: center\">\r\n        <v-btn\r\n          :disabled=\"!valid\"\r\n          color=\"normal\"\r\n          class=\"mr-4\"\r\n          @click=\"validate\"\r\n        >\r\n          Login\r\n        </v-btn>\r\n        <v-btn @click=\"$router.push('/signup')\">Sign up</v-btn>\r\n      </div>\r\n\r\n      <!-- <v-btn color=\"error\" class=\"mr-4\" @click=\"reset\">\r\n        Reset Form\r\n      </v-btn> -->\r\n    </v-form>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data: () => ({\r\n    valid: true,\r\n    name: \"\",\r\n    nameRules: [\r\n      (v) => !!v || \"Name is required\",\r\n      (v) => (v && v.length <= 10) || \"Name must be less than 10 characters\",\r\n    ],\r\n    password: \"\",\r\n    passwordRules: [\r\n      (v) => !!v || \"Password is required\",\r\n      (v) => (v && v.length >= 8) || \"Password must be more than 8 characters\",\r\n    ],\r\n    select: null,\r\n    items: [\"Item 1\", \"Item 2\", \"Item 3\", \"Item 4\"],\r\n    checkbox: false,\r\n  }),\r\n\r\n  methods: {\r\n    validate() {\r\n      const isValid = this.$refs.form.validate();\r\n      if (isValid) {\r\n        this.$router.push(\"/\");\r\n      }\r\n    },\r\n    reset() {\r\n      this.$refs.form.reset();\r\n      this.$refs.form.resetValidation();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"sourceRoot":"src/views"}]}