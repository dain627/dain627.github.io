\n\r\n  methods: {\r\n    validate() {\r\n      const isValid = this.$refs.form.validate();\r\n      if (isValid) {\r\n        this.$router.push(\"/\");\r\n      }\r\n    },\r\n    reset() {\r\n      this.$refs.form.reset();\r\n      this.$refs.form.resetValidation();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"]}]}