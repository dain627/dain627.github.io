A,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,UAAA,OAAA,GAAA,KAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EAAA;;AACA,UAAA,OAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,KANA;AAOA,IAAA,KAPA,mBAOA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,eAAA;AACA;AAVA;AA1BA,CAAA","sourcesContent":["<template>\r\n  <v-container>\r\n    <v-row class=\"my-6\">\r\n      <v-col cols=\"12\" style=\"text-align: center\">\r\n        <h1>Register</h1>\r\n      </v-col>\r\n    </v-row>\r\n    <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\r\n      <v-text-field\r\n        v-model=\"name\"\r\n        :rules=\"nameRules\"\r\n        label=\"User Name\"\r\n        required\r\n        outlined\r\n      ></v-text-field>\r\n\r\n      <v-text-field\r\n        v-model=\"password\"\r\n        :rules=\"passwordRules\"\r\n        label=\"Password\"\r\n        type=\"password\"\r\n        required\r\n        outlined\r\n      ></v-text-field>\r\n\r\n      <v-text-field\r\n        v-model=\"firstName\"\r\n        :rules=\"requiredRules\"\r\n        label=\"First Name\"\r\n        required\r\n        outlined\r\n      ></v-text-field>\r\n\r\n      <v-text-field\r\n        v-model=\"lastName\"\r\n        :rules=\"requiredRules\"\r\n        label=\"Last Name\"\r\n        required\r\n        outlined\r\n      ></v-text-field>\r\n      <v-text-field\r\n        v-model=\"email\"\r\n        :rules=\"emailRules\"\r\n        label=\"Email\"\r\n        required\r\n        outlined\r\n      ></v-text-field>\r\n      <v-text-field\r\n        v-model=\"contact\"\r\n        :rules=\"requiredRules\"\r\n        label=\"Contact Number\"\r\n        required\r\n        outlined\r\n      ></v-text-field>\r\n\r\n      <v-select\r\n        v-model=\"select\"\r\n        :items=\"items\"\r\n        :rules=\"[(v) => !!v || 'Item is required']\"\r\n        label=\"Member Type\"\r\n        outlined\r\n        required\r\n      ></v-select>\r\n\r\n      <div style=\"text-align: center\">\r\n        <v-btn\r\n          :disabled=\"!valid\"\r\n          color=\"normal\"\r\n          class=\"mr-4\"\r\n          @click=\"validate\"\r\n        >\r\n          Sign up\r\n        </v-btn>\r\n      </div>\r\n\r\n      <!-- <v-btn color=\"error\" class=\"mr-4\" @click=\"reset\">\r\n        Reset Form\r\n      </v-btn> -->\r\n    </v-form>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data: () => ({\r\n    valid: true,\r\n    name: \"\",\r\n    nameRules: [\r\n      (v) => !!v || \"Name is required\",\r\n      (v) => (v && v.length <= 10) || \"Name must be less than 10 characters\",\r\n    ],\r\n    password: \"\",\r\n    passwordRules: [\r\n      (v) => !!v || \"Password is required\",\r\n      (v) => (v && v.length >= 8) || \"Password must be more than 8 characters\",\r\n    ],\r\n    emailRules: [\r\n      (v) => !!v || \"E-mail is required\",\r\n      (v) => /.+@.+\\..+/.test(v) || \"E-mail must be valid\",\r\n    ],\r\n    requiredRules: [(v) => !!v || \"This field is required\"],\r\n    firstName: \"\",\r\n    lastName: \"\",\r\n    email: \"\",\r\n    contact: \"\",\r\n    select: null,\r\n    items: [\"Chef\", \"Customer\"],\r\n  }),\r\n\r\n  methods: {\r\n    validate() {\r\n      const isValid = this.$refs.form.validate();\r\n      if (isValid) {\r\n        this.$router.push(\"/\");\r\n      }\r\n    },\r\n    reset() {\r\n      this.$refs.form.reset();\r\n      this.$refs.form.resetValidation();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"sourceRoot":"src/views"}]}